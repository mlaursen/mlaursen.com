{
  "name": "mlaursen.com",
  "homepage": "https://mlaursen.com",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,scss,md,yml,json}\"",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "predeploy": "rm -rf out && yarn build-static && touch out/.nojekyll && cp CNAME out/CNAME",
    "deploy": "gh-pages -d out -t",
    "test-cy": "cypress open",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build-static": "next build && next export"
  },
  "repository": "git@github.com:mlaursen/mlaursen.com.git",
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "dependencies": {
    "next": "^12.1.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-md": "^5.1.2"
  },
  "devDependencies": {
    "@mlaursen/eslint-config": "^1.6.0",
    "@testing-library/cypress": "^8.0.2",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "cypress": "^9.6.0",
    "eslint": "^8.14.0",
    "eslint-config-next": "^12.1.6",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "typescript": "^4.6.4"
  },
  "lint-staged": {
    "**/*.{md,ts,tsx,scss,js,yml}": [
      "prettier --write"
    ]
  }
}
