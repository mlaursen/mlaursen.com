    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "playwright": "^1.43.0",
    "prettier": "^3.2.5",
    "sass": "^1.74.1",
    "serve": "^14.2.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4"
