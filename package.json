{
  "name": "mlaursen.com",
  "homepage": "https://mlaursen.com",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,scss,md,yml,json,svelte}\"",
    "check": "svelte-check --tsconfig ./tsconfig.json",
