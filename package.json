{
  "name": "mlaursen.com",
  "type": "module",
  "homepage": "https://mlaursen.com",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,scss,md,yml,json}\"",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\"",
    "test-cy": "cypress open",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start-static": "serve -p 3000 ./out"
  },
  "engines": {
    "pnpm": ">=8",
    "node": ">=18"
  },
  "engineStrict": true,
  "repository": "git@github.com:mlaursen/mlaursen.com.git",
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "dependencies": {
    "@react-md/core": "1.0.0-next.3",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@mlaursen/eslint-config": "^4.0.0",
    "@testing-library/cypress": "^10.0.1",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "cypress": "^13.6.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "husky": "^8.0.1",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "sass": "^1.69.7",
    "serve": "^14.2.1",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "**/*.{ts,tsx,scss,js,jsx,md,yml,json}": [
      "prettier --write"
    ]
  }
}
