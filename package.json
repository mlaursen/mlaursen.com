{
  "name": "mlaursen.com",
  "homepage": "https://mlaursen.com",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "predeploy": "rm -rf out && next build && next export && touch out/.nojekyll && cp CNAME out/CNAME",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "deploy": "gh-pages -d out -t",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install"
  },
  "repository": "git@github.com:mlaursen/mlaursen.com.git",
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "dependencies": {
    "@react-md/link": "^2.8.2",
    "@react-md/theme": "^2.8.2",
    "@react-md/typography": "^2.8.2",
    "@react-md/utils": "^2.8.2",
    "next": "^10.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@mlaursen/eslint-config": "^1.1.3",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "eslint": "^7.26.0",
    "gh-pages": "^3.1.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "**/*.md": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx,scss}": [
      "prettier --write"
    ]
  }
}
