  "private": true,
  "scripts": {
    "predeploy": "rm -rf out && next build && next export && touch out/.nojekyll && cp CNAME out/CNAME",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "deploy": "gh-pages -d out -t",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install"
  },
  "repository": "git@github.com:mlaursen/mlaursen.com.git",
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "dependencies": {
    "next": "^12.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-md": "^5.1.0"
  },
  "devDependencies": {
    "@mlaursen/eslint-config": "^1.6.0",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "eslint": "^8.12.0",
    "eslint-config-next": "^12.1.1",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "prettier": "^2.5.1",
    "sass": "^1.50.0",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "**/*.{md,ts,tsx,scss,js,yml}": [
      "prettier --write"
    ]
  }
