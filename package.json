  "private": true,
  "scripts": {
    "predeploy": "rm -rf out && next build && next export && touch out/.nojekyll && cp CNAME out/CNAME",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "deploy": "gh-pages -d out -t",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install"
  },
  "repository": "git@github.com:mlaursen/mlaursen.com.git",
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "dependencies": {
    "next": "^11.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-md": "^3.1.1"
  },
  "devDependencies": {
    "@mlaursen/eslint-config": "^1.1.5",
    "@types/node": "^16.10.4",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "**/*.{md,ts,tsx,scss,js,yml}": [
      "prettier --write"
    ]
  }
