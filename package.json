{
  "name": "mlaursen.com",
  "type": "module",
  "homepage": "https://mlaursen.com",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,scss,md,yml,json}\"",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\"",
    "test-cy": "cypress open",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start-static": "npx serve -p 3000 ./out"
  },
  "engines": {
    "pnpm": ">=8",
    "node": ">=18"
  },
  "engineStrict": true,
  "repository": "git@github.com:mlaursen/mlaursen.com.git",
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "dependencies": {
    "@react-md/core": "^1.0.0-next.0",
    "next": "^13.4.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@mlaursen/eslint-config": "^2.4.0",
    "@testing-library/cypress": "^9.0.0",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "cypress": "^12.17.1",
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.10",
    "husky": "^8.0.1",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "**/*.{ts,tsx,scss,js,jsx,md,yml,json}": [
      "prettier --write"
    ]
  }
