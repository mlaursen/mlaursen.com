{
  "name": "mlaursen.com",
  "homepage": "https://mlaursen.com",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "predeploy": "rm -rf out && next build && next export && touch out/.nojekyll && cp CNAME out/CNAME",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "deploy": "gh-pages -d out -t",
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "repository": "git@github.com:mlaursen/mlaursen.com.git",
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "dependencies": {
    "@react-md/link": "^2.4.2",
    "@react-md/theme": "^2.4.1",
    "@react-md/typography": "^2.4.2",
    "@react-md/utils": "^2.4.2",
    "next": "^10.0.1",
    "react": "^16.14.0",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@mlaursen/eslint-config": "^0.0.4",
    "@types/node": "^14.11.10",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "eslint": "^7.11.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "**/*.md": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx,scss}": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "node-fetch": "2.6.1",
    "webpack": "5.0.0"
  }
}
