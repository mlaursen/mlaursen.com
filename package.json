    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@mlaursen/eslint-config": "^4.1.1",
    "@playwright/test": "^1.42.1",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.22",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "playwright": "^1.42.1",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
