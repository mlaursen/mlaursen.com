{
  "name": "mlaursen.com",
  "type": "module",
  "homepage": "https://mlaursen.com",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,scss,md,yml,json}\"",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\"",
    "test-cy": "cypress open",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start-static": "npx serve -p 3000 ./out"
  },
  "engines": {
    "pnpm": ">=7",
    "node": ">=18"
  },
  "engineStrict": true,
  "repository": "git@github.com:mlaursen/mlaursen.com.git",
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "dependencies": {
    "next": "^13.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-md": "^5.1.5"
  },
  "devDependencies": {
    "@mlaursen/eslint-config": "^2.3.0",
    "@testing-library/cypress": "^9.0.0",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "cypress": "^12.14.0",
    "eslint": "^8.42.0",
    "eslint-config-next": "^13.4.6",
    "husky": "^8.0.1",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "sass": "^1.63.4",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "**/*.{ts,tsx,scss,js,jsx,md,yml,json}": [
      "prettier --write"
    ]
  }
